      showClose: true,\n            message: '无权访问！',\n            type: 'error'\n          });\n        });\n      }, 200);\n    },\n    getUserListByPage: function getUserListByPage() {\n      var _this2 = this;\n      this.loading = true;\n      setTimeout(function () {\n        _this2.$store.dispatch('GetUserListByPage', _this2.tablePage).then(function (res) {\n          var statusCode = res.data.statusCode;\n          // 判断结果\n          if (statusCode == 200) {\n            _this2.data.list = res.data.data;\n            _this2.data.listCopy = _this2.data.list;\n            _this2.loading = false;\n          } else {\n            console.log(\"错误\");\n          }\n        }).catch(function (err) {\n          console.log(err);\n          _this2.$message({\n            showClose: true,\n            message: '无权访问！',\n            type: 'error'\n          });\n        });\n      }, 200);\n    },\n    searchEvent: function searchEvent() {\n      var filterName = XEUtils.toValueString(this.data.filterName).trim().toLowerCase();\n      if (filterName) {\n        var filterRE = new RegExp(filterName, 'gi');\n        var searchProps = ['nickName', 'username'];\n        var rest = this.data.tableData.filter(function (item) {\n          return searchProps.some(function (key) {\n            return XEUtils.toValueString(item[key]).toLowerCase().indexOf(filterName) > -1;\n          });\n        });\n        this.data.list = rest.map(function (row) {\n          var item = Object.assign({}, row);\n          searchProps.forEach(function (key) {\n            item[key] = XEUtils.toValueString(item[key]).replace(filterRE, function (match) {\n              return \"<span class=\\\"keyword-lighten\\\">\".concat(match, \"</span>\");\n            });\n          });\n          return item;\n        });\n      } else {\n        this.data.list = this.data.listCopy;\n      }\n    },\n    handlePageChange: function handlePageChange(_ref) {\n      var currentPage = _ref.currentPage,\n       