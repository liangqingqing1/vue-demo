      }]\n      },\n      formData: {\n        // 弹出的表单数据\n        username: '',\n        nickName: '',\n        mobile: '',\n        roleName: '',\n        onboardingTime: '',\n        departureTime: '',\n        age: '',\n        homeAddress: '',\n        status: '',\n        role: '',\n        roleList: [{\n          roleId: 1,\n          roleName: \"超级管理员\"\n        }, {\n          roleId: 5,\n          roleName: \"董事会\"\n        }, {\n          roleId: 6,\n          roleName: \"出纳\"\n        }, {\n          roleId: 7,\n          roleName: \"采购员\"\n        }, {\n          roleId: 8,\n          roleName: \"销售员\"\n        }, {\n          roleId: 9,\n          roleName: \"财务\"\n        }, {\n          roleId: 11,\n          roleName: \"业务员\"\n        }, {\n          roleId: 13,\n          roleName: \"物流操作\"\n        }]\n      }\n    };\n  },\n  methods: {\n    insertEvent: function insertEvent() {\n      this.showEdit = true, this.selectRow = null;\n    },\n    editEvent: function editEvent(row) {\n      this.selectRow = row;\n      this.showEdit = true;\n      this.formData = {\n        username: row.username,\n        nickName: row.nickName,\n        mobile: row.mobile,\n        roleName: row.roleName,\n        onboardingTime: row.onboardingTime,\n        departureTime: row.departureTime,\n        age: row.age,\n        homeAddress: row.homeAddress,\n        status: row.status,\n        role: row.role\n      };\n    },\n    getUserList: function getUserList() {\n      var _this = this;\n      this.loading = true;\n      setTimeout(function () {\n        _this.$store.dispatch('GetUserList').then(function (res) {\n          var statusCode = res.data.statusCode;\n          // 判断结果\n          if (statusCode == 200) {\n            _this.data.tableData = res.data.data;\n            _this.data.list = res.data.data;\n            _this.tablePage.total = res.data.data.length;\n            _this.loading = false;\n          } else {\n            console.log(\"错误\");\n          }\n        }).catch(function () {\n          _this.$message({\n            showClose: true,\n            message: '无权访问！',\n            type: 'error'\n          });\n        });\n      }, 200);\n    },\n    getUserListByPage: function getUserListByPage() {\n      var _this2 = this;\n      this.loading = true;\n      setTimeout(function () {\n        _this2.$store.dispatch('GetUserListByPage', _this2.tablePage).then(function (res) {\n          var statusCode = res.data.statusCode;\n          // 判断结果\n          if (statusCode == 200) {\n            _this2.data.list = res.data.data;\n            _this2.data.listCopy = _this2.data.list;\n            _this2.loading = false;\n          } else {\n            console.log(\"错误\");\n          }\n        }).catch(function (err) {\n          console.log(err);\n          _this2.$message({\n            showClose: true,\n            message: '无权访问！',\n            type: 'error'\n          });\n        });\n      }, 200);\n    },\n    searchEvent: function searchEvent() {\n      var filterName = XEUtils.toValueString(this.data.filterName).trim().toLowerCase();\n      if (filterName) {\n        var filterRE = new RegExp(filterName, 'gi');\n        var searchProps = ['nickName', 'username'];\n        var rest = this.data.tableData.filter(function (item) {\n          return searchProps.some(function (key) {\n            return XEUtils.toValueString(item[key]).toLowerCase().indexOf(filterName) > -1;\n          });\n        });\n        this.data.list = rest.map(function (row) {\n          var item = Object.assign({}, row);\n          searchProps.forEach(function (key) {\n            item[key] = XEUtils.toValueString(item[key]).replace(filterRE, function (match) {\n              return \"<span class=\\\"keyword-lighten\\\">\".concat(match, \"</span>\");\n            });\n          });\n          return item;\n        });\n      } else {\n        this.data.list = this.data.listCopy;\n      }\n    },\n    handlePageChange: function hand